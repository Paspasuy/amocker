{
  "name": "AmogusBlock",
  "version": "1.0",
  "description": "My amogus adblocker",
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
  "background": {
	"scripts": ["background.js"],
	"persistent": true
  },

  "icons": {
    "48": "amogus.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
